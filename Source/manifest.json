{
	"manifest_version": 2,
	"name": "Imgur to Gfycat",
	"version": "1.0",
	"description": "Replaces imgur gifs with gfycat optimized videos and gifs.",
	"permissions": [
		"history"
	],
	"web_accessible_resources": ["gfyembed.js"],
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["superagent.js", "content_script.js"],
			"run_at": "document_end"
		}
	]
}

